extends layout

block content
  .w3-container
    .w3-card-4
      header.w3-container.w3-red.w3-margin-top
        h3 CLAV - Classificação e Avaliação da Informação Pública
    .w3-card-4
      header.w3-container.w3-red.w3-margin-top
        h4 Lista de Classe 1
      table.w3-table-all
        tr
          th Titulo
          th Codigo
        each tip in lista
                tr
                    td= tip.titulo
                    td
                      a(href= "http://localhost:5666/classe/" +tip.codigo)= tip.codigo
                    
    footer.w3-container.w3-red Gerado por Mariana Pereira :: 2021-Janeiro
        

